	<dependencies>
		<dependency>
			<groupId>com.hybris.datahub</groupId>
			<artifactId>datahub-dto</artifactId>
			<version>${datahub.version}</version>
		</dependency>
		<dependency>
			<groupId>com.hybris.datahub</groupId>
			<artifactId>datahub-rest-client</artifactId>
			<version>${datahub.version}</version>
		</dependency>
		<dependency>
